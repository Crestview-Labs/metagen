    "react": "^18.0.0",
    "react-devtools-core": "^4.28.5",
    "string-width": "^7.2.0",
    "strip-ansi": "^7.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.23",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.0.0",
    "typescript": "^5.3.0"
