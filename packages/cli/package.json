{
  "name": "@metagen/cli",
  "version": "0.1.0",
  "description": "TypeScript CLI for metagen agent",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "metagen": "dist/index.js"
  },
  "scripts": {
